{
  "name": "lighthouse-plugin-user-flow-metrics",
  "author": "Richard Laffers",
  "license": "MIT",
  "version": "2.0.0",
  "exports": "./plugin.js",
  "type": "module",
  "scripts": {
    "lint": "eslint plugin.js audits/",
    "lint:fix": "eslint --fix plugin.js audits/",
    "format": "prettier --write README.md plugin.js audits/**/*.{js,mjs,cjs}",
    "format:check": "prettier --check README.md plugin.js audits/**/*.{js,mjs,cjs}",
    "prepare": "husky install",
    "commit": "cz"
  },
  "files": [
    "package.json",
    "plugin.js",
    "README.md",
    "audits/*.js",
    "LICENSE"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "lighthouse": "^10.1.0",
    "prettier": "^2.7.1"
  },
  "peerDependencies": {
    "lighthouse": "^10.0.2"
  },
  "volta": {
    "node": "18.14.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": " >=14.13.1 || >=16.0.0"
  }
}
